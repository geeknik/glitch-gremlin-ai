{
  "name": "@glitch-gremlin/sdk",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
"main": "./dist/cjs/index.js",
"module": "./dist/esm/index.js",
"types": "./dist/esm/index.d.ts",
"exports": {
".": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js", 
    "types": "./dist/esm/index.d.ts"
}
},
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "node -e \"require('dotenv').config()\"",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "clean": "rm -rf dist",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --maxWorkers=4 --detectOpenHandles",
    "test:ci": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --maxWorkers=4 --runInBand --detectOpenHandles --ci --reporters=default --reporters=jest-junit",
    "test:security": "npm audit --production --audit-level=moderate && snyk test",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "audit": "npm audit --production --audit-level=moderate"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "dotenv": "^16.4.7",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.7.3",
    "ts-node": "^10.9.2"
  }
}
