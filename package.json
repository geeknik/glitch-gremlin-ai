{
"name": "glitch-gremlin-ai",
"version": "0.1.0",
"packageManager": "npm@10.8.2",
"description": "AI-powered chaos testing for Solana dApps",
"type": "module",
"scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.ts",
    "test:ai": "jest --config jest.config.ts 'sdk/src/ai/__tests__/.*\\.test\\.ts$'",
    "test:watch": "jest --config jest.config.ts --watch",
    "lint": "eslint src",
    "start": "node build/src/index.js", 
    "clean": "rimraf build coverage",
    "prepare": "husky install"
},
"workspaces": [
    "sdk/*"
],
"dependencies": {
    "@solana/web3.js": "^1.87.1",
    "@tensorflow/tfjs": "^4.12.0",
    "@tensorflow/tfjs-node": "^4.12.0",
    "commander": "^11.0.0",
    "redis": "^4.6.10"
},
"devDependencies": {
    "@jest/types": "^29.6.3",
    "@swc/core": "^1.3.95",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
}
}
