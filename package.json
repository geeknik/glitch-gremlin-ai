{
  "name": "glitch-gremlin-ai",
  "version": "0.1.0",
  "packageManager": "npm@10.8.2",
  "description": "AI-powered chaos testing for Solana dApps",
  "type": "module",
  "bin": {
    "glitch": "./cli/dist/index.js"
  },
  "scripts": {
    "build": "npm run build:sdk && npm run build:cli",
    "build:sdk": "tsc -p sdk/tsconfig.json",
    "build:cli": "tsc -p cli/tsconfig.json --skipLibCheck",
    "test": "jest --config jest.config.ts",
    "test:ai": "jest --config jest.config.ts 'sdk/src/ai/__tests__/.*\\.test\\.ts$'",
    "test:watch": "jest --config jest.config.ts --watch",
    "lint": "eslint src",
    "start": "node build/src/index.js",
    "clean": "rimraf build coverage",
    "prepare": "husky install"
  },
  "workspaces": [
    "sdk/*"
  ],
  "dependencies": {
    "@glitch-gremlin/sdk": "file:sdk",
    "@tensorflow/tfjs": "^4.12.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "commander": "^11.0.0",
    "execa": "^9.5.2",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@solana/web3.js": "^1.98.0",
    "@swc/core": "^1.3.95",
    "@swc/jest": "^0.2.29",
    "@tensorflow/tfjs-node": "^3.21.1",
    "@types/bn.js": "^5.1.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.16",
    "@types/socket.io": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^8.0.3",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "zod": "^3.24.1"
  }
}
