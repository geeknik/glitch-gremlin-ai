    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@solana/web3.js": "^1.89.1",
    "@tensorflow/tfjs-node": "^4.22.0",
    "bn.js": "^6.0.0",
